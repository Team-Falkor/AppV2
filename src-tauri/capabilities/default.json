{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:app:default",
    "shell:allow-open",
    "fs:default",
    "fs:allow-create",
    "fs:allow-appdata-read",
    "fs:allow-appdata-write",
    "core:path:default",
    "core:path:allow-resolve-directory",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*"
        }
      ]
    },
    "sql:default",
    "sql:allow-execute",
    "sql:allow-load",
    "sql:allow-select",
    "store:default"
  ]
}
